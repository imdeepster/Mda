        <servlet-class>com.Action.ViewData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>verifyData</servlet-name>
        <servlet-class>com.zot.verifyData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientLoginAction</servlet-name>
        <servlet-class>com.Action.ClientLoginAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MailVerify</servlet-name>
        <servlet-class>com.Action.MailVerify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataOwnerLoginAction</servlet-name>
        <url-pattern>/DataOwnerLoginAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OwnerRegistration</servlet-name>
        <url-pattern>/OwnerRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientDataEntryAction</servlet-name>
        <url-pattern>/ClientDataEntryAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServerLogin</servlet-name>
        <url-pattern>/ServerLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetKey</servlet-name>
        <url-pattern>/GetKey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewData</servlet-name>
        <url-pattern>/ViewData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>verifyData</servlet-name>
        <url-pattern>/verifyData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientLoginAction</servlet-name>
        <url-pattern>/ClientLoginAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MailVerify</servlet-name>
        <url-pattern>/MailVerify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
